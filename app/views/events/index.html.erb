<h2>Current Events</h2>

<div class="events">
  <%= render @current_events %>
</div>

<h2>Closed Events</h2>

<div class="events">
  <%= render @closed_events %>
</div>

<div>
  <h2>NYT Events</h2>

  <%= @nyt_events_query.status %><br />
  <%= @nyt_events_query.num_results %><br />
  <%= @nyt_events_query.events.inspect %><br />
  <% @nyt_events_query.events.each do |event| %>
    <%= event.event_name %><br />
    <%= event.some_fake_field.inspect %><br />
  <% end %>
</div>
